<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE validators PUBLIC 
  		"-//OpenSymphony Group//XWork Validator 1.0.2//EN" 
  		"http://www.opensymphony.com/xwork/xwork-validator-1.0.2.dtd">

  	<validators>	
  		<field name="fname">
					<field-validator type="requiredstring">
						<message>Valid FirstName is required</message>
					</field-validator>
		</field>
				
		<field name="lname">
					<field-validator type="requiredstring">
						<message>Valid LastName is required</message>
					</field-validator>
		</field>
				
		<field name="certificate">
					<field-validator type="requiredstring">
						<message>certificate feild must not be empty</message>
					</field-validator>
		</field>
		
		<field name="specailist">
					<field-validator type="requiredstring">
						<message>specailist is required</message>
					</field-validator>
		</field>
		
		
	</validators>