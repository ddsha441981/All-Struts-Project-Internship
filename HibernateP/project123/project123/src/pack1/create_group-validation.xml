<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE validators PUBLIC 
  		"-//OpenSymphony Group//XWork Validator 1.0.2//EN" 
  		"http://www.opensymphony.com/xwork/xwork-validator-1.0.2.dtd">
  <validators>
  
            <field name="group_id">
  			<field-validator type="requiredstring">
  			<message>Group ID field is Empty</message>
  			</field-validator>
  			<field-validator type="int">
  			<message>Group ID must be in Number</message>
  			</field-validator>
  	        </field>
  			
  	        <field name="intern_id">
  			<field-validator type="requiredstring">
  			<message>Intern ID field is Empty</message>
  			</field-validator>
  			<field-validator type="int">
  			<message>Intern ID must be in Number</message>
  			</field-validator>
  	        </field>
  	   
  	        <field name="project_name">
  			<field-validator type="requiredstring">
  			<message>Project Name is Required</message>
  			</field-validator>
  		    </field>
  
  </validators>