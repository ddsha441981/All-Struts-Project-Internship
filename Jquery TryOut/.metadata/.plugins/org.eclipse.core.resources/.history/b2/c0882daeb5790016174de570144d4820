    <script>
$(document).ready( function(){
	setTimeout( function(){
		$('.terminal').typewriting( "jQuery TypeWriting.js Demo", {
			"typing_interval": 200,
			"blink_interval": "1s",
			"cursor_color": "#00fd55"
		}, function() {
			console.log( "END" );
		});
		setTimeout( function(){
			$('.terminal').rewrite( "This is jQuery TypeWriting.js", function(){
				console.log( "END, 2." );
			});
		}, 3300);
	}, 1000);
});

</script>
